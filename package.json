{
  "name": "anylist-cli",
  "version": "0.0.1",
  "author": {
    "name": "Michael Mior",
    "email": "michael.mior@gmail.com",
    "url": "https://michael.mior.ca/"
  },
  "license": "MIT",
  "homepage": "https://github.com/michaelmior/anylist-cli",
  "repository": "https://github.com/michaelmior/anylist-cli",
  "bugs": "https://github.com/michaelmior/anylist-cli/issues",
  "bin": {
    "anylist": "./src/cli.js"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "flow focus-check",
      "prettier --write"
    ],
    "package.json": [
      "npmPkgJsonLint -q .",
      "pkg-okg"
    ]
  },
  "scripts": {
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "flow": "flow check .",
    "lint": "prettier --check src/**/*.js && eslint && npmPkgJsonLint -q . && pkg-ok",
    "test": "jest"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  },
  "dependencies": {
    "anylist": "^0.8.4",
    "commander": "^13.1.0",
    "cosmiconfig": "^8.3.6",
    "enquirer": "^2.4.1"
  },
  "devDependencies": {
    "coveralls": "^3.1.1",
    "eslint": "^9.19.0",
    "flow-bin": "^0.259.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.4.3",
    "mock-fs": "^5.5.0",
    "npm-package-json-lint": "^8.0.0",
    "npm-package-json-lint-config-default": "^7.0.1",
    "pkg-ok": "^3.0.0",
    "prettier": "^3.5.0"
  }
}
